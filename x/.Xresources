! Molokai theme
! See: https://gist.github.com/vreon/845878
*.foreground:   #d0cfcf
*.background:   #191915
*.cursorColor:  #d0cfcf

! black
*.color0:       #272822
*.color8:       #3a3b36

! red
*.color1:       #9f0c4b
*.color9:       #f0058a

! green
*.color2:       #66aa11
*.color10:      #80ff00

! yellow
*.color3:       #c47f2c
*.color11:      #ffba68

! blue
*.color4:       #008fe2
*.color12:      #0074b7

! magenta
*.color5:       #7e40a5
*.color13:      #bb88dd

! cyan
*.color6:       #3579a8
*.color14:      #4eb4fa

! white
*.color7:       #9999aa
*.color15:      #d0d0d0


urxvt*font: xft:Fira Code:size=10, \
            xft:WenQuanYi Bitmap Song:size=8, \
            xft:FreeSerif:style=Regular, \
            xft:unifont:style=Medium:antialias=false
urxvt*scrollBar: false
urxvt*depth: 32
urxvt.internalBorder: 20
! urxvt*background: rgba:0000/0000/0200/c800

Xft.dpi: 192
Xft.autohint: 0
Xft.lcdfilter:  lcddefault
Xft.hintstyle:  hintfull
Xft.hinting: 1
Xft.antialias: 1
Xft.rgba: rgb
